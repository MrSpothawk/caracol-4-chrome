{
  	"name": "__MSG_appName__",
  	"description": "__MSG_appDescription__",
  	"version": "1",
  	"manifest_version": 2,
    "default_locale": "en",
  	"permissions": [
      "http://caracol.cloudapp.net/",
      "http://api.readability.com/", 
      "bookmarks", 
      "cookies"
    ],
    "icons":{
      "16": "images/caracol4.png",
      "128": "images/icon-128.png"
    },
    "browser_action": {
      "default_icon": {
        "19": "images/caracol4.png",
        "38": "images/caracol3.png"
      },
      "default_title": "caracol 4 chrome",
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "chrome_url_overrides" : {
      "newtab": "tab.html"
    }
    
  }