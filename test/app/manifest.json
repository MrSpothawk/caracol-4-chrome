{
  	"name": "__MSG_appName__",
  	"description": "__MSG_appDescription__",
  	"version": "1",
  	"manifest_version": 2,
  	"default_locale": "en",
  	"permissions": [
  		{
  			"socket": [
  				"tcp-listen:*:*",
  				"tcp-connect:*:*"
  			]
  		},
  		"adview (dev)",
  		"app.runtime",
  		"app.window",
  		"clipboardWrite",
  		"geolocation",
  		"infobars (dev)",
  		"location (dev)",
  		"webview",
  		"<all_urls>",
  		"http://*/*",
  		"https://*/*",
  		"http://127.0.0.1/*",
  		"chrome-extension://*/*"
  	]
  }